<h1>Uncover the secrets of our Licornes</h1>

<% if @licornes.any? %>
  <% @licornes.each do |licorne| %>
      <%= link_to licorne_path(licorne) do %>
          <%= licorne.name %>
          <%= licorne.price %>
      <% end %>
  <% end %>
<% end %>

<%= link_to "Add a unicorn", new_licorne_path %>
